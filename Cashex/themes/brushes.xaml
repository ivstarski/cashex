<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="Primary" Color="#FF0F6C98" />
    <SolidColorBrush x:Key="PrimaryDark" Color="#FF0D394E" />
    <SolidColorBrush x:Key="PrimaryLight" Color="#C5CAE9" />
    <SolidColorBrush x:Key="PrimaryText" Color="Orange" />
    <SolidColorBrush x:Key="SecondaryText" Color="#FF00D1FF" />
    <LinearGradientBrush x:Key="Secondary" StartPoint="0,0" EndPoint="0,1">
        <GradientStop Offset="0" Color="#FFF3F3F3" />
        <GradientStop Offset="0.5" Color="#FFEBEBEB" />
        <GradientStop Offset="0.5" Color="#FFDDDDDD" />
        <GradientStop Offset="1" Color="White" />
    </LinearGradientBrush>
    <SolidColorBrush x:Key="Accent" Color="White" />
    <Color x:Key="PrimaryColor">#FF0D394E</Color>
</ResourceDictionary>